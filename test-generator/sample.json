{
    "name": "dotnetcore-sample",
    "plugins": {
        "nuget-pkg|assemblyname.dll": "1.0.x"
    },
    "tests": [{
        "name": "Sanity Check",
        "type": "Namespace.InMemoryTestClass",
        "input": "in.json",
        "output": "out.json",
        "outputFormatter": "ConsoleOutputFormatterClass"
    }, {
        "name": "Test One Kafka",
        "type": "KafkaTestClass",
        "input": {
            "type": "plugin",
            "value": "InputGeneratorClass"
        },
        "output": "out.json",
        "options": {
            "auth_token": "1234"
        }
    }]
}
